/* 弹出层*/
.tan_wrap{position: fixed;left: 0;bottom: -700rpx;opacity: 0;width: 100%;height: 700rpx;transition: all 0.5s;-webkit-transition: all 0.5s;z-index: 111;}
.tan_wrap .close{position: absolute;right: 12rpx;top: 50%;transform: translateY(-50%);-webkit-transform: translateY(-50%);width: 44rpx;height: 44rpx;padding: 15rpx;}
.tan_wrap .tan-wrap-max-height{overflow:scroll;max-height:550rpx;}



