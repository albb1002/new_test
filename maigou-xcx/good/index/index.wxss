@import "../../template/wxss/common.wxss";
.head-left-icon{width: 43rpx;height: 51rpx;position: relative;}
.head-left-icon image{width: 100%;height: 20rpx;position: absolute;top: 0}
.head-left-icon-txt{position: absolute;width: 100%;bottom: 0;text-align: center;}
.head-search-txt{padding: 18rpx 0;text-indent: 65rpx;}
.head-search-txt image{width: 33rpx;height: 33rpx;position: absolute;left: 22rpx;top: 50%;transform: translateY(-50%);-webkit-transform: translateY(-50%)}
.head-wrap{position: fixed;width: 100%;left: 0;top: 0;z-index: 111}

.good-tab-icon{width: 80rpx;height: 40rpx;}
.good-tab-icon image{width: 40rpx;height: 40rpx;}

.price-nav .px-price{width: 30rpx;height: 30rpx;transform: rotate(-90deg);position: relative;top: 4rpx;transition: all 0.5s;}
.price-nav .px-price-atv{transform: rotate(90deg);}
.good-list-img{width: 200rpx;min-width: 200rpx;height: 200rpx;}
.good-list-img image{width: 100%;height: 100%}
.coupon-tip{display: inline-block;padding:6rpx 15rpx; border: 1px solid}
.good-info{position: absolute;bottom: 10rpx;left: 27rpx;}


.good-info-lh{line-height: 40rpx}
.list-type-2{margin-right: 2%;margin-bottom: 15rpx;max-width: 49%;min-width: 49%;}
.list-type-2:nth-child(2n){margin-right: 0;}
.type-2-img image{width: 100%;height: 100%}
.type-2-img{height: 312rpx;min-height: 312rpx;width: 100%}

.aside-nav{position: fixed;width: 550rpx;height: 100%;z-index: 111;top: 0;left: -550rpx;transition: all 0.5s}

/* 加载中 */
.loading{margin: auto;width: 130rpx;overflow: hidden;animation: load 3s linear infinite;-webkit-animation: load 3s linear infinite;white-space: nowrap}
@keyframes load{
  0%{width:130rpx}
  24.5%{width:130rpx}
  25%{width:115rpx}
  49.5%{width:115rpx}
  50%{width:100rpx;}
  74.5%{width:100rpx;}
  75%{width:85rpx;}
  100%{width:85rpx;}
}

.checked{width: 30rpx;height: 26rpx;}
.two-list{text-indent: 52rpx;}
.check-list2{width: 26rpx;height: 26rpx;}
.check-atv{transform: rotate(90deg)}
.slide-up{width: 100rpx;}

.search-close-img{right: 15rpx;left: auto !important;padding: 15rpx;}



